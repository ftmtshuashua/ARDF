<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:orientation="vertical"
    android:layout_height="match_parent">


    <include
        android:id="@+id/layout_ActionBar"
        layout="@layout/layout_actionbar" />


    <com.lfp.ardf.widget.WebViewFk
        android:id="@+id/view_WebView"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/layout_ActionBar" />


    <com.lfp.androidrapiddevelopmentframework.widget.WebProgressBar
        android:id="@+id/view_WebProgressBar"
        android:layout_width="match_parent"
        android:layout_height="2dp"  />
</android.support.constraint.ConstraintLayout>